#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/apple/Desktop/2024-2025_processing/寒假/lce_monitor/node_modules/.pnpm/sass@1.78.0/node_modules/sass/node_modules:/Users/apple/Desktop/2024-2025_processing/寒假/lce_monitor/node_modules/.pnpm/sass@1.78.0/node_modules:/Users/apple/Desktop/2024-2025_processing/寒假/lce_monitor/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/apple/Desktop/2024-2025_processing/寒假/lce_monitor/node_modules/.pnpm/sass@1.78.0/node_modules/sass/node_modules:/Users/apple/Desktop/2024-2025_processing/寒假/lce_monitor/node_modules/.pnpm/sass@1.78.0/node_modules:/Users/apple/Desktop/2024-2025_processing/寒假/lce_monitor/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/sass@1.78.0/node_modules/sass/sass.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/sass@1.78.0/node_modules/sass/sass.js" "$@"
fi
