#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/apple/Desktop/2024-2025_processing/寒假/lce_monitor/node_modules/.pnpm/vite@6.1.1_@types+node@22.13.4_jiti@2.4.2_sass@1.78.0_terser@5.39.0_yaml@2.7.0/node_modules/vite/bin/node_modules:/Users/apple/Desktop/2024-2025_processing/寒假/lce_monitor/node_modules/.pnpm/vite@6.1.1_@types+node@22.13.4_jiti@2.4.2_sass@1.78.0_terser@5.39.0_yaml@2.7.0/node_modules/vite/node_modules:/Users/apple/Desktop/2024-2025_processing/寒假/lce_monitor/node_modules/.pnpm/vite@6.1.1_@types+node@22.13.4_jiti@2.4.2_sass@1.78.0_terser@5.39.0_yaml@2.7.0/node_modules:/Users/apple/Desktop/2024-2025_processing/寒假/lce_monitor/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/apple/Desktop/2024-2025_processing/寒假/lce_monitor/node_modules/.pnpm/vite@6.1.1_@types+node@22.13.4_jiti@2.4.2_sass@1.78.0_terser@5.39.0_yaml@2.7.0/node_modules/vite/bin/node_modules:/Users/apple/Desktop/2024-2025_processing/寒假/lce_monitor/node_modules/.pnpm/vite@6.1.1_@types+node@22.13.4_jiti@2.4.2_sass@1.78.0_terser@5.39.0_yaml@2.7.0/node_modules/vite/node_modules:/Users/apple/Desktop/2024-2025_processing/寒假/lce_monitor/node_modules/.pnpm/vite@6.1.1_@types+node@22.13.4_jiti@2.4.2_sass@1.78.0_terser@5.39.0_yaml@2.7.0/node_modules:/Users/apple/Desktop/2024-2025_processing/寒假/lce_monitor/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
